{"ast":null,"code":"import { ref } from \"vue\";\nimport HomeHeader from \"../components/HomeHeader.vue\";\n\n// 定义一个响应式变量，用于存储是否有 token\n\nexport default {\n  __name: 'EmojiView',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const hastoken = ref(false);\n\n    // 判断 localStorage 中是否有 token\n    hastoken.value = !!localStorage.getItem(\"token\");\n    const __returned__ = {\n      hastoken,\n      ref,\n      HomeHeader\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","HomeHeader","hastoken","value","localStorage","getItem"],"sources":["D:/_xhf/java/html_css_javascript/vue/vuerouter/vue-school-travel-app/src/views/EmojiView.vue"],"sourcesContent":["<template>\n  <div>\n    <!-- 如果有 token 显示 \"1\"，否则显示 HomeHeader -->\n    <template v-if=\"hastoken\">\n      <div>\n        <h1>用户管理表格</h1>\n\n        <!-- 历史更改信息和确认区 -->\n        <div class=\"status-container\">\n          <div id=\"statusMessage\" class=\"status-message\"></div>\n          <div id=\"deleteConfirm\" class=\"delete-confirm\">\n            <span>确认删除此用户？</span>\n            <button class=\"confirm-btn\" onclick=\"deleteRow()\">确认</button>\n            <button class=\"cancel-btn\" onclick=\"cancelDelete()\">取消</button>\n          </div>\n        </div>\n\n        <!-- 表格区 -->\n        <div class=\"table-container\">\n          <table>\n            <thead>\n              <tr>\n                <th>序号</th>\n                <th>姓名</th>\n                <th>邮箱</th>\n                <th>电话</th>\n                <th>身份</th>\n                <th>状态</th>\n                <th>操作类型</th>\n              </tr>\n            </thead>\n            <tbody id=\"userTable\">\n              <tr>\n                <td>1</td>\n                <td>张三</td>\n                <td>zhangsan@example.com</td>\n                <td>123-456-7890</td>\n                <td>学生</td>\n                <td>\n                  <label class=\"toggle-switch\">\n                    <input\n                      type=\"checkbox\"\n                      onchange=\"toggleStatus(this, '张三')\"\n                    />\n                    <span class=\"slider\"></span>\n                  </label>\n                </td>\n                <td>\n                  <span class=\"action-btn edit-btn\" onclick=\"editRow(this)\"\n                    >✏️</span\n                  >\n                  <span\n                    class=\"action-btn delete-btn\"\n                    onclick=\"confirmDelete(this)\"\n                    >🗑️</span\n                  >\n                </td>\n              </tr>\n              <!-- 更多数据 -->\n            </tbody>\n          </table>\n        </div>\n      </div>\n    </template>\n    <template v-else>\n      <HomeHeader></HomeHeader>\n      <div class=\"stress\">请先注册或登录再使用相关功能</div>\n    </template>\n  </div>\n</template>\n\n<script setup>\nimport { ref } from \"vue\";\nimport HomeHeader from \"../components/HomeHeader.vue\";\n\n// 定义一个响应式变量，用于存储是否有 token\nconst hastoken = ref(false);\n\n// 判断 localStorage 中是否有 token\nhastoken.value = !!localStorage.getItem(\"token\");\n</script>\n\n<style scoped>\n.stress {\n  position: absolute;\n  top: 50%;\n  left: 50%;\n  transform: translate(-50%, -50%);\n  font-size: 24px;\n  color: red;\n}\nbody {\n  font-family: Arial, sans-serif;\n  margin: 0;\n  padding: 20px;\n  background: linear-gradient(135deg, #d4f7dc, #a8e9f5);\n  min-height: 100vh;\n}\n\nh1 {\n  text-align: center;\n  color: #333;\n}\n\n.status-container {\n  width: 90%;\n  max-width: 1200px;\n  margin: 0 auto 20px;\n  padding: 10px;\n  background: #ffffff;\n  border-radius: 8px;\n  box-shadow: 0 4px 10px rgba(0, 0, 0, 0.1);\n}\n\n.status-message {\n  font-size: 16px;\n  margin: 5px 0;\n  text-align: center;\n  color: #333;\n}\n\n.status-message.visible {\n  color: #4caf50;\n  font-weight: bold;\n}\n\n.delete-confirm {\n  display: none;\n  text-align: center;\n}\n\n.delete-confirm button {\n  margin: 0 10px;\n  padding: 8px 16px;\n  font-size: 14px;\n  border: none;\n  border-radius: 4px;\n  cursor: pointer;\n}\n\n.delete-confirm .confirm-btn {\n  background-color: #e63946;\n  color: white;\n}\n\n.delete-confirm .cancel-btn {\n  background-color: #ccc;\n  color: #333;\n}\n\n.table-container {\n  margin: 0 auto;\n  width: 90%;\n  max-width: 1200px;\n  border-radius: 8px;\n  overflow: hidden;\n  box-shadow: 0 4px 10px rgba(0, 0, 0, 0.1);\n  background-color: #ffffff;\n}\n\ntable {\n  width: 100%;\n  border-collapse: collapse;\n}\n\nthead {\n  background-color: #4caf50;\n  color: white;\n}\n\nth,\ntd {\n  padding: 12px;\n  text-align: center;\n  border: 1px solid #ddd;\n}\n\ntbody tr:nth-child(even) {\n  background-color: #f9f9f9;\n}\n\ntbody tr:hover {\n  background-color: #f1f1f1;\n}\n\n/* 确保 th 不受 tr:hover 的影响 */\nthead th {\n  background-color: #4caf50;\n  color: white;\n}\n\n.toggle-switch {\n  position: relative;\n  display: inline-block;\n  width: 40px;\n  height: 20px;\n}\n\n.toggle-switch input {\n  display: none;\n}\n\n.slider {\n  position: absolute;\n  cursor: pointer;\n  top: 0;\n  left: 0;\n  right: 0;\n  bottom: 0;\n  background-color: #ccc;\n  transition: 0.4s;\n  border-radius: 20px;\n}\n\n.slider:before {\n  position: absolute;\n  content: \"\";\n  height: 14px;\n  width: 14px;\n  left: 3px;\n  bottom: 3px;\n  background-color: white;\n  transition: 0.4s;\n  border-radius: 50%;\n}\n\ninput:checked + .slider {\n  background-color: #4caf50;\n}\n\ninput:checked + .slider:before {\n  transform: translateX(20px);\n}\n\n.action-btn {\n  cursor: pointer;\n  font-size: 18px;\n  color: #333;\n  transition: color 0.3s ease;\n}\n\n.action-btn:hover {\n  color: #4caf50;\n}\n\n.delete-btn {\n  color: #e63946;\n}\n\n.delete-btn:hover {\n  color: #c4001d;\n}\n\n.edit-row input,\n.edit-row select {\n  width: 90%;\n  padding: 5px;\n  font-size: 14px;\n}\n</style>\n"],"mappings":"AAwEA,SAASA,GAAG,QAAQ,KAAK;AACzB,OAAOC,UAAU,MAAM,8BAA8B;;AAErD;;;;;;;;IACA,MAAMC,QAAQ,GAAGF,GAAG,CAAC,KAAK,CAAC;;IAE3B;IACAE,QAAQ,CAACC,KAAK,GAAG,CAAC,CAACC,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}