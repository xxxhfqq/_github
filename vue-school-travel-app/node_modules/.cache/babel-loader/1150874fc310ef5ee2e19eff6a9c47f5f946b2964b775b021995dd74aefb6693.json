{"ast":null,"code":"import \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.for-each.js\";\nimport { ref } from \"vue\";\nimport HomeHeader from \"../components/HomeHeader.vue\";\n\n// 定义一个响应式变量，用于存储是否有 token\n\nexport default {\n  __name: 'EmojiView',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const hastoken = ref(false);\n\n    // 判断 localStorage 中是否有 token\n    hastoken.value = !!localStorage.getItem(\"token\");\n    function selectEmoji(emoji) {\n      // 移除所有表情的选中状态\n      const emojis = document.querySelectorAll(\".emoji\");\n      emojis.forEach(function (el) {\n        el.classList.remove(\"selected\");\n      });\n\n      // 为当前点击的表情添加选中状态\n      const selectedEmoji = document.querySelector(`.emoji[onclick=\"selectEmoji('${emoji}')\"]`);\n      selectedEmoji.classList.add(\"selected\");\n\n      // 将点击的表情的文字描述填充到隐藏的 input 字段\n      document.getElementById(\"selectedEmoji\").value = emoji;\n\n      // 启用提交按钮\n      document.getElementById(\"submitButton\").disabled = false;\n    }\n    const __returned__ = {\n      hastoken,\n      selectEmoji,\n      ref,\n      HomeHeader\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","HomeHeader","hastoken","value","localStorage","getItem","selectEmoji","emoji","emojis","document","querySelectorAll","forEach","el","classList","remove","selectedEmoji","querySelector","add","getElementById","disabled"],"sources":["D:/_xhf/java/html_css_javascript/vue/vuerouter/vue-school-travel-app/src/views/EmojiView.vue"],"sourcesContent":["<template>\n  <div>\n    <!-- 如果有 token 显示 \"1\"，否则显示 HomeHeader -->\n    <template v-if=\"hastoken\">\n      <div class=\"body\">\n        <div class=\"emoji-container\">\n          <form action=\"/submit-emoji\" method=\"POST\">\n            <h2>你现在的心情如何？</h2>\n            <!-- 美观的提示文字 -->\n\n            <div class=\"emoji-group\">\n              <div class=\"emoji\" @click=\"selectEmoji('confused')\">😕</div>\n              <div class=\"emoji\" @click=\"selectEmoji('angry')\">😡</div>\n              <div class=\"emoji\" @click=\"selectEmoji('laughing')\">😆</div>\n              <div class=\"emoji\" @click=\"selectEmoji('sleepy')\">😴</div>\n              <div class=\"emoji\" @click=\"selectEmoji('worried')\">😨</div>\n              <div class=\"emoji\" @click=\"selectEmoji('smiling')\">🙂</div>\n              <div class=\"emoji\" @click=\"selectEmoji('disappointed')\">😞</div>\n              <div class=\"emoji\" @click=\"selectEmoji('smirking')\">😏</div>\n            </div>\n\n            <!-- 隐藏字段用于存储选中的表情的描述 -->\n            <input type=\"hidden\" id=\"selectedEmoji\" name=\"emoji\" value=\"\" />\n\n            <!-- 提交按钮 -->\n            <button type=\"submit\" class=\"submit-btn\" id=\"submitButton\" disabled>\n              确认提交\n            </button>\n          </form>\n        </div>\n      </div>\n    </template>\n    <template v-else>\n      <HomeHeader></HomeHeader>\n      <div class=\"stress\">请先注册或登录再使用相关功能</div>\n    </template>\n  </div>\n</template>\n\n<script setup>\nimport { ref } from \"vue\";\nimport HomeHeader from \"../components/HomeHeader.vue\";\n\n// 定义一个响应式变量，用于存储是否有 token\nconst hastoken = ref(false);\n\n// 判断 localStorage 中是否有 token\nhastoken.value = !!localStorage.getItem(\"token\");\nfunction selectEmoji(emoji) {\n  // 移除所有表情的选中状态\n  const emojis = document.querySelectorAll(\".emoji\");\n  emojis.forEach(function (el) {\n    el.classList.remove(\"selected\");\n  });\n\n  // 为当前点击的表情添加选中状态\n  const selectedEmoji = document.querySelector(\n    `.emoji[onclick=\"selectEmoji('${emoji}')\"]`\n  );\n  selectedEmoji.classList.add(\"selected\");\n\n  // 将点击的表情的文字描述填充到隐藏的 input 字段\n  document.getElementById(\"selectedEmoji\").value = emoji;\n\n  // 启用提交按钮\n  document.getElementById(\"submitButton\").disabled = false;\n}\n</script>\n\n<style scoped>\n.stress {\n  position: absolute;\n  top: 50%;\n  left: 50%;\n  transform: translate(-50%, -50%);\n  font-size: 24px;\n  color: red;\n}\n.body {\n  font-family: Arial, sans-serif;\n  background: linear-gradient(135deg, #a8d5ba, #e4f5e1); /* 渐变淡绿色背景 */\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  height: 100vh;\n  margin: 0;\n}\n.emoji-container {\n  background-color: rgba(255, 255, 255, 0.9); /* 半透明白色背景 */\n  padding: 30px;\n  border-radius: 12px;\n  box-shadow: 0 10px 20px rgba(0, 0, 0, 0.1);\n  text-align: center;\n  width: 80%;\n  max-width: 600px;\n}\n.emoji-group {\n  display: grid;\n  grid-template-columns: repeat(4, 1fr); /* 每行4个表情 */\n  gap: 20px;\n  margin-bottom: 20px;\n}\n.emoji {\n  font-size: 3rem;\n  cursor: pointer;\n  padding: 15px;\n  border-radius: 50%;\n  transition: transform 0.2s ease, box-shadow 0.2s ease;\n  background-color: #fff;\n  border: 2px solid transparent;\n}\n.emoji:hover {\n  transform: scale(1.2);\n  box-shadow: 0 4px 10px rgba(0, 0, 0, 0.2);\n}\n.emoji.selected {\n  border: 2px solid #4caf50; /* 选中表情的边框颜色 */\n  background-color: #e8f5e9; /* 选中表情的背景颜色 */\n}\n.emoji:active {\n  transform: scale(1);\n  box-shadow: 0 2px 5px rgba(0, 0, 0, 0.2);\n}\n.submit-btn {\n  background-color: #4caf50;\n  color: white;\n  border: none;\n  padding: 15px 32px;\n  font-size: 18px;\n  cursor: pointer;\n  border-radius: 5px;\n  transition: background-color 0.3s ease;\n  width: 100%;\n}\n.submit-btn:disabled {\n  background-color: #ddd;\n  cursor: not-allowed;\n}\n.submit-btn:hover:enabled {\n  background-color: #45a049;\n}\nh2 {\n  font-size: 24px;\n  margin-bottom: 20px;\n  color: #333;\n}\n</style>\n"],"mappings":";;AAwCA,SAASA,GAAG,QAAQ,KAAK;AACzB,OAAOC,UAAU,MAAM,8BAA8B;;AAErD;;;;;;;;IACA,MAAMC,QAAQ,GAAGF,GAAG,CAAC,KAAK,CAAC;;IAE3B;IACAE,QAAQ,CAACC,KAAK,GAAG,CAAC,CAACC,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC;IAChD,SAASC,WAAWA,CAACC,KAAK,EAAE;MAC1B;MACA,MAAMC,MAAM,GAAGC,QAAQ,CAACC,gBAAgB,CAAC,QAAQ,CAAC;MAClDF,MA