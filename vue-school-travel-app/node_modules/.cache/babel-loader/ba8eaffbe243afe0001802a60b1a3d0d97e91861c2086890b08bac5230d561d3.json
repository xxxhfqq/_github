  margin: 0;\n  padding: 20px;\n  background: linear-gradient(135deg, #d4f7dc, #a8e9f5);\n  min-height: 100vh;\n}\n\nh1 {\n  text-align: center;\n  color: #333;\n}\n\n.status-container {\n  width: 90%;\n  max-width: 1200px;\n  margin: 0 auto 20px;\n  padding: 10px;\n  background: #ffffff;\n  border-radius: 8px;\n  box-shadow: 0 4px 10px rgba(0, 0, 0, 0.1);\n}\n\n.status-message {\n  font-size: 16px;\n  margin: 5px 0;\n  text-align: center;\n  color: #333;\n}\n\n.status-message.visible {\n  color: #4caf50;\n  font-weight: bold;\n}\n\n.delete-confirm {\n  display: none;\n  text-align: center;\n}\n\n.delete-confirm button {\n  margin: 0 10px;\n  padding: 8px 16px;\n  font-size: 14px;\n  border: none;\n  border-radius: 4px;\n  cursor: pointer;\n}\n\n.delete-confirm .confirm-btn {\n  background-color: #e63946;\n  color: white;\n}\n\n.delete-confirm .cancel-btn {\n  background-color: #ccc;\n  color: #333;\n}\n\n.table-container {\n  margin: 0 auto;\n  width: 90%;\n  max-width: 1200px;\n  border-radius: 8px;\n  overflow: hidden;\n  box-shadow: 0 4px 10px rgba(0, 0, 0, 0.1);\n  background-color: #ffffff;\n}\n\ntable {\n  width: 100%;\n  border-collapse: collapse;\n}\n\nthead {\n  background-color: #4caf50;\n  color: white;\n}\n\nth,\ntd {\n  padding: 12px;\n  text-align: center;\n  border: 1px solid #ddd;\n}\n\ntbody tr:nth-child(even) {\n  background-color: #f9f9f9;\n}\n\ntbody tr:hover {\n  background-color: #f1f1f1;\n}\n\n/* 确保 th 不受 tr:hover 的影响 */\nthead th {\n  background-color: #4caf50;\n  color: white;\n}\n\n.toggle-switch {\n  position: relative;\n  display: inline-block;\n  width: 40px;\n  height: 20px;\n}\n\n.toggle-switch input {\n  display: none;\n}\n\n.slider {\n  position: absolute;\n  cursor: pointer;\n  top: 0;\n  left: 0;\n  right: 0;\n  bottom: 0;\n  background-color: #ccc;\n  transition: 0.4s;\n  border-radius: 20px;\n}\n\n.slider:before {\n  position: absolute;\n  content: \"\";\n  height: 14px;\n  width: 14px;\n  left: 3px;\n  bottom: 3px;\n  backgroun